{
  "name": "uchion",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"VITE,API\" --prefix-colors \"cyan,magenta\"",
    "dev:frontend": "vite --host",
    "dev:backend": "vercel dev --listen 3000",
    "dev:full": "vercel dev",
    "build": "vite build",
    "preview:network": "vite preview --host",
    "smoke": "tsx scripts/smoke-generate.ts",
    "preview": "vite preview",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:run && npm run test:e2e"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^3.3.4",
    "@pdf-lib/fontkit": "^1.1.1",
    "@tanstack/react-query": "^5.50.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "openai": "^4.58.1",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.13.0",
    "postgres": "^3.4.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "react-router-dom": "^6.23.0",
    "zod": "^3.23.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^22.9.0",
    "@types/pdfkit": "^0.17.4",
    "@types/supertest": "^6.0.3",
    "@vercel/node": "^3.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "postcss": "^8.4.31",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.16"
  }
}
